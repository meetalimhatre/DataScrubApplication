VIEW "CDS_DS.V_PRC_CONFIG_ENTITY" AS SELECT
  T_PRC_CONFIGURATIONS_0.PRC_ID,
  T_PRC_CONFIGURATIONS_0.CONFIGURATION_NAME,
  T_PRC_CONFIGURATIONS_0.RULESET_NAME,
  T_PRC_CONFIGURATIONS_0.ENTITY_GROUP,
  T_PRC_ENTITIES_1.ENTITY_NAME
FROM ("CDS_DS.T_PRC_CONFIGURATIONS" AS T_PRC_CONFIGURATIONS_0 LEFT JOIN "CDS_DS.T_PRC_ENTITIES" AS T_PRC_ENTITIES_1 ON T_PRC_CONFIGURATIONS_0.PRC_ID = T_PRC_ENTITIES_1.PRC_ID AND T_PRC_CONFIGURATIONS_0.ENTITY_GROUP = T_PRC_ENTITIES_1.ENTITY_GROUP)