COLUMN TABLE "CDS_DS.T_PRC_CONFIGURATIONS" (
  PRC_ID BIGINT NOT NULL,
  CONFIGURATION_NAME NVARCHAR(100) NOT NULL,
  RULESET_NAME NVARCHAR(100) NOT NULL DEFAULT '',
  ENTITY_GROUP NVARCHAR(100) NOT NULL DEFAULT '',
  CONF_STATUS NVARCHAR(30),
