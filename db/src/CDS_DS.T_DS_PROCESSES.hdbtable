COLUMN TABLE "CDS_DS.T_DS_PROCESSES" (
  PRC_ID BIGINT NOT NULL,
  DATA_SCRUB_NAME NVARCHAR(100),
  DATA_SCRUB_DESC NVARCHAR(300),
  COMPANY_CODE NVARCHAR(64),
  START_DATE DATE,
  END_DATE DATE,
  STATUS_ID NVARCHAR(30),
  CREATED_AT SECONDDATE,
  OWNER NVARCHAR(100),
  CONFIGURATION_NAME NVARCHAR(100),
  USER_GROUP NVARCHAR(80),
  ITEM_SET NVARCHAR(100),
  PRIMARY KEY(PRC_ID)
)