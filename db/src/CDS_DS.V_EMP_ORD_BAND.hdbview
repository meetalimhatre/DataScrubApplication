VIEW "CDS_DS.V_EMP_ORD_BAND" AS SELECT
  T_EMP_ORDER_0.PERNR AS PERSONNEL_ID,
  T_EMP_ORDER_0.BEGDA AS VALID_FROM,
  T_EMP_ORDER_0.ENDDA AS VALID_TO,
  T_EMP_ORDER_0.AUFNR AS ORDER_NUMER,
  T_EMP_ORDER_0.KTEXT AS ORDER_DESC,
  T_EMP_BAND_1.BAND_LEVEL AS BAND_LEVEL,
  T_EMP_BAND_1.STEXT AS JOB_DESC
FROM ("CDS_DS.T_EMP_ORDER" AS T_EMP_ORDER_0 INNER JOIN "CDS_DS.T_EMP_BAND" AS T_EMP_BAND_1 ON T_EMP_ORDER_0.PERNR = T_EMP_BAND_1.PERNR)