  V_PROC_EXPENSES_0.ITEM_SET,
  V_PROC_EXPENSES_0.REPORT_ID,
  V_PROC_EXPENSES_0.ALLOCATION_KEY,
  V_PROC_EXPENSES_0.EMPLOYEE_ID,
  V_PROC_EXPENSES_0.EMPLOYEE_NAME,
