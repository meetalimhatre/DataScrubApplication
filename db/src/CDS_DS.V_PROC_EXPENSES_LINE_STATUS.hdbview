VIEW "CDS_DS.V_PROC_EXPENSES_LINE_STATUS" AS SELECT
  V_PROC_EXPENSES_0.DATA_SCRUB_DESC,
  V_PROC_EXPENSES_0.PRC_ID,
  V_PROC_EXPENSES_0.DATA_SCRUB_NAME,
  V_PROC_EXPENSES_0.COMPANY_CODE,
  V_PROC_EXPENSES_0.START_DATE,
  V_PROC_EXPENSES_0.END_DATE,
