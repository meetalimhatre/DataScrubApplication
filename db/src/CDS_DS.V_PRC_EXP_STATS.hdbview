VIEW "CDS_DS.V_PRC_EXP_STATS" AS SELECT
  V_EXP_PROC_DISP_HEADER_0.PRC_ID,
  1 AS SAMPLE_COUNT,
  V_EXP_PROC_DISP_HEADER_0.TOTAL_APPROVED_AMOUNT,
  CASE WHEN V_EXP_PROC_DISP_HEADER_0.SYS_STATUS = 'SYSTEM_EXCLUDED' THEN V_EXP_PROC_DISP_HEADER_0.TOTAL_APPROVED_AMOUNT ELSE 0 END AS SYSTEM_EXCLUDED_AMOUNT,
  CASE WHEN V_EXP_PROC_DISP_HEADER_0.REP_STATUS = 'REVIEWER_EXCLUDED' THEN V_EXP_PROC_DISP_HEADER_0.TOTAL_APPROVED_AMOUNT ELSE 0 END AS MANUAL_EXCLUDED_AMOUNT
FROM "CDS_DS.V_EXP_PROC_DISP_HEADER" AS V_EXP_PROC_DISP_HEADER_0