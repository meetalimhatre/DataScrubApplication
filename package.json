{
  "name": "DataScrubApplication",
  "description": "Reference application built according to the application programming model for SAP Cloud Platform",
  "version": "1.0.0",
  "dependencies": {
    "@sap/cds": "2.x",
    "@sap/hana-client": "^2.11.14"
  },
  "devDependencies": {},
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "build": "cds build --clean"
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana",
        "model": "db/"
      },
      "uaa": {
        "kind": "xsuaa"
      }
    }
  },
  "private": true
}