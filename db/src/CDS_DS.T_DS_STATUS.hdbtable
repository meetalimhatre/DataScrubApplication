COLUMN TABLE "CDS_DS.T_DS_STATUS" (
  PRC_ID BIGINT NOT NULL,
  REPORT_ID NVARCHAR(64) NOT NULL,
  REPORT_STATUS NVARCHAR(30),
  SYSTEM_STATUS NVARCHAR(30),
  PRIORITY NVARCHAR(100),
  MANUAL_STATUS NVARCHAR(30),
  MATCHING_RULES NVARCHAR(1000),
  MATCHING_ENTITIES NVARCHAR(1000),
  PRIMARY KEY(PRC_ID, REPORT_ID)
)