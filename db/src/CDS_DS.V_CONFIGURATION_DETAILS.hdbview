VIEW "CDS_DS.V_CONFIGURATION_DETAILS" AS SELECT
  T_CONFIGURATIONS_0.CONFIGURATION_NAME,
  T_CONFIGURATIONS_0.ENTITY_GROUP,
  T_ENTITIES_1.ENTITY_NAME
FROM ("CDS_DS.T_CONFIGURATIONS" AS T_CONFIGURATIONS_0 INNER JOIN "CDS_DS.T_ENTITIES" AS T_ENTITIES_1 ON T_CONFIGURATIONS_0.ENTITY_GROUP = T_ENTITIES_1.ENTITY_GROUP)