VIEW "CDS_DS.V_ALLOCATION_CODES" AS SELECT
  T_ALLOCATION_CODES_0.ALLOCATION_CODE,
  T_ALLOCATION_CODES_0.ALLOCATED_COMPANY_CODE,
  T_ALLOCATION_CODES_0.FISCPER,
  T_ALLOCATION_CODES_0.DATE_OF_ENTRY,
  month(T_ALLOCATION_CODES_0.DATE_OF_ENTRY) AS DATE_MONTH,
  year(T_ALLOCATION_CODES_0.DATE_OF_ENTRY) AS DATE_YEAR
FROM "CDS_DS.T_ALLOCATION_CODES" AS T_ALLOCATION_CODES_0